<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    @*@Styles.Render("~/Content/extjs")
    @Scripts.Render("~/bundles/extjs")*@

    <link rel="stylesheet" type="text/css" href="~/Scripts/extjs/resources/css/ext-all.css">
    <link rel="stylesheet" type="text/css" href="~/Scripts/PersonApp/styles/style.css">
    <script type="text/javascript" src="~/Scripts/extjs/ext-all-debug.js"></script>
    <script type="text/javascript" src="~/Scripts/app.js"></script>
</head>
<body>
    @*<script src="~/Scripts/PersonApp/data/data.js"></script>*@

    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Scripts", required: false)
</body>
</html>

